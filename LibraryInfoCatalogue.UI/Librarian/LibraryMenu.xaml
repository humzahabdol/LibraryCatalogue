<Page x:Class="LibraryInfoCatalogue.UI.LibraryMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LibraryInfoCatalogue.UI"
      mc:Ignorable="d" 
      d:DesignHeight="507" d:DesignWidth="965"
      Title="LibraryMenu">

    <Grid x:Name="containerCanvas" Margin="0,0,0,9.5">
        <Frame x:Name="frm_gotomenu" Content="Frame" HorizontalAlignment="Left" Height="507" Margin="0,0,0,-9.5" VerticalAlignment="Top" Width="965"/>
